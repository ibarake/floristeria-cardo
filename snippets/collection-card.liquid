<a class="collection-card" href="{{ collection.url }}">
  <img
    class="collection-card-image"
    src="{{ collection.featured_image | img_url }}"
    alt="{{ collection.featured_image.alt }}">
  <p class="subtitulo-3 collection-card-title">
    {{ collection.title }}
    {% render 'icon-arrow-right-2' %}
  </p>
</a>